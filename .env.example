# 数据库配置示例
# 复制此文件为 .env 并填入实际配置

# PostgreSQL数据库配置
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=your_password_here
DB_NAME=tikuadapter

# 数据库连接池配置
DB_POOL_SIZE=10
DB_MAX_OVERFLOW=20

# 是否启用SQL日志（开发环境可以设置为true）
DB_ECHO=false

# 日志配置
# 日志级别：DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO
# 日志保留天数
LOG_BACKUP_DAYS=30
AUTH_ENABLED=false